n<-as.integer(readline(prompt = "Enter limit: "))
a<-0
b<-1
print(a)
print(b)
for ( i in 0:n )
{
  c=a+b
  print(c)
  a<-b
  b<-c
}

n<-as.integer(readline(prompt = "Enter limit: "))
a<-0
b<-1
count<-2
if(n==1){
  print(a)
} else {
print("Fibonacci Series")
print(a)
print(b)
    while(count<n){
         c<-a+b
         print(c)
         a<-b
         b<-c
        count<-count+1
  }
}

